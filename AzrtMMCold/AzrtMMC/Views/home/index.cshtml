                            <!-- Data cpunt section end -->
                            <!-- Blog section start -->
                            <section class="elementor-element elementor-element-a1ca601 elementor-section-stretched elementor-section-boxed elementor-section-height-default elementor-section-height-default elementor-section elementor-top-section"
                                     data-id="a1ca601"
                                     data-element_type="section"
                                     data-settings='{"background_background":"classic","stretch_section":"section-stretched"}'>
                                <!-- Start History Section -->
                                <section id="history" class="history elementor-element mb-4">
                                    <!-- Section Inner -->
                                    <div class="section-inner container">

                                        <!-- Section Title -->
                                        <h2 class="awards section-title blog-header">
                                            Bloglar
                                        </h2>
                                        <!-- Section Description -->
                                        <p class="section-title">
                                            Məhsul və xidmətlərimiz bizimlə bağlı maraqlı bütün məlumatları əldə etmək üçün bloqumuza daxil ola bilərsiniz
                                        </p>

                                        <!-- History dates and notes -->
                                        <div class="history-wrapper clearfix animated" data-animation="fadeInUp" data-animation-delay="100">

                                            <!-- Keeper when loading new items -->
                                            <div class="note-keeper">

                                                <!-- Date, Start Date -->
                                                <span class="d-none date start-date">@ViewBag.DateTime</span>
                                                @foreach (var blog in Model.Blogs)
                                                {
                                                    <!-- Note -->
                                                    <div class="note animated" data-animation="fadeInUp" data-animation-delay="200">
                                                        <!-- Note Details -->
                                                        <div class="note-inner">
                                                            <!-- Note Image -->
                                                            <div class="note-image">
                                                                @if (!String.IsNullOrEmpty(blog.ImagePath))
                                                                {
                                                                    <img src="~/wwwroot/blog/@blog.ImagePath" alt="@blog.Title">
                                                                }
                                                            </div>
                                                            <!-- Note Texts -->
                                                            <div class="note-texts">
                                                                <!-- Note Date -->
                                                                <p class="d-none note-date">@blog.CreatedDate.ToShortDateString()</p>
                                                                <!-- Note Title -->
                                                                <h4 class="note-title">@blog.Title</h4>
                                                                <!-- Note Desc -->
                                                                <p class="note-desc">
                                                                    @if (blog.Description.Length > 101)
                                                                    {
                                                                        @Html.Raw(blog.Description.Substring(0, 100)); <span>...</span>
                                                                    }
                                                                    else
                                                                    {
                                                                        @Html.Raw(blog.Description);
                                                                    }
                                                                </p>
                                                            </div>
                                                            <!-- End Note Texts -->
                                                        </div>
                                                        <!-- End Note Details -->
                                                    </div>
                                                    <!-- End Note -->
                                                }
                                                <div class="news">
                                                    <a href="/blogs" class="load-more-button font-secondary">
                                                        <span>Bloglar</span>
                                                    </a>
                                                </div>
                                                <!-- Keeper when loading new items -->

                                            </div>
                                            <!-- End History dates and notes -->

                                        </div>
                                    </div>
                                    <!-- End Section Inner -->

                                </section>
                                <!-- End History Section -->
                            </section>
                            <!-- Blog section end -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
