                                    <style>
                                        /*.tb-site-header.tb-color2 .tb-primary-nav .tb-primary-nav-list>.menu-item>a:hover{
                                            color:#fff;
                                        }*/
                                        .list-unstyledd {
                                            height: 30px;
                                            width: 50px;
                                           color:#ffffffad;
                                           padding-top:19px;
                                        }
                                        .list-unstyledd:hover{
                                            color:#fff !important;
                                        }

                                            .list-unstyledd li {
                                                padding: 5px 16px;
                                                z-index: 2;
                                            }

                                                .list-unstyledd li:not(.init) {
                                                    float: left;
                                                    width: 50px;
                                                    display: none;
                                                    background: #fafafa;
                                                    color:black;
                                                   
                                                }

                                                    .list-unstyledd li:not(.init):hover , .list-unstyledd li.selected:not(.init) {
                                                        background: #09f;
                                                        color:#fafafa;
                                                    }
                                                     .list-unstyledd li:not(.init) a:hover , .list-unstyledd li.selected:not(.init) a{
                                                        color:#fafafa;
                                                    }

                                        li.init {
                                            cursor: pointer;
                                        }

                                        a#submit {
                                            z-index: 1;
                                        }
                                    </style>
                                    <script>
                                        $(".list-unstyledd").on("click", ".init", function () {
                                            $(this).closest(".list-unstyledd").children('li:not(.init)').toggle();
                                        });

                                        var allOptions = $(".list-unstyledd").children('li:not(.init)');
                                        $(".list-unstyledd").on("click", "li:not(.init)", function () {
                                            allOptions.removeClass('selected');
                                            $(this).addClass('selected');
                                            $(".list-unstyledd").children('.init').html($(this).html());
                                            allOptions.toggle();
                                        });
                                    </script>
                                </li>
                            </ul>
