                                    <ul class="list-unstyledd">
                                        <li class="init d-flex">@AzrtMMC.Resources.HeaderTexts.EN<i class="fas fa-sort-down pl-2"></i></li>
                                        <li data-value="value 1"><a href="/language/index?lang=Az&route=@ViewBag.RouteController/@ViewBag.RouteAction">AZ</a></li>
                                        <li data-value="value 2"><a href="/language/index?lang=En&route=@ViewBag.RouteController/@ViewBag.RouteAction">EN</a></li>
